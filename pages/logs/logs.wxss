page{
    width: 100%;
    height: 100%;
}
.container{
    height: 100%;
    background: #f9f7fa;
}
.lcenter{
    position: absolute;
    left: 50%;
}
.container .pic{
    width: 120px;
    height:120px;
}
.container .log-box{
    background: #f9f7fa;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}
.log-box .userInfo{
    border: 1px solid #d7d7d7;
    width: 60px;
    height: 60px;
    border-radius: 60px;
    background: #fff;
    -webkit-background-size:contain;
    background-size:contain;
    /*margin-top: 50px;*/
    margin: 20px auto 0;
    z-index:3;
}
.container .log-list{
    border: 0;
    z-index:3;
}
.log-list .content-box{
    background: #fff;
    border: 1px solid #d7d7d7;
    width: 180px;
    /*height: 150px;*/
    margin: 0 auto;
    border-radius: 10px;
    font-size: 16px;
    padding: 20px;
    line-height: 20px;

}
.log-list .content-box.notext{
    padding: 0 20px;
    height: 30px;
    line-height: 30px;
}
.content-box .date-time{
    text-align: center;
}
.content-box .text{
    font-size: 18px;
    line-height: 24px;
    margin-top: 20px;
}
.log-list .busy{
    border: 1px solid #d7d7d7;
    width: 40px;
    height: 40px;
    border-radius: 40px;
    background: #fff;
    margin: 0 auto;
    font-size: 20px;
    line-height: 40px;
    text-align: center;
}


.log-box .line{
    border-right:1px solid #d7d7d7;
    width: 1px;
    height: 20px;
    margin: 0 auto;
    z-index: 1;
}